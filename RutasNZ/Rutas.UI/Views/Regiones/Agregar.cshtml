@model Rutas.UI.Models.AgregarRegionVista

@{
}


<h1 class="mt-3">Agregar Region</h1>

<form method="post">

    <div class="mt-3">
        <label class="form-label">Codigo</label>
        <input type="text" class="form-control" asp-for="Codigo" />
    </div>

    <div class="mt-3">
        <label class="form-label">Nombre</label>
        <input type="text" class="form-control" asp-for="Nombre" />
    </div>

    <div class="mt-3">
        <label class="form-label">ImagenUrl</label>
        <input type="text" class="form-control" asp-for="ImagenRegionUrl" />
    </div>

    <div class="mt-3">
        <button type="submit" class="btn btn-primary">Guardar</button>
    </div>

</form>
